<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    cq:showOnCreate="{Boolean}false"
    jcr:primaryType="nt:unstructured"
    jcr:title="Advanced"
    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
        <column
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured" 
                sling:hideChildren="[section1,templates,authenticationrequirement,export]">
                <basic
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Page-Configuration"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <language
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/select"
                            class="language"
                            cq-msm-lockable="jcr:language"
                            fieldLabel="Page-Language"
                            fieldDescription="Page-Language description"
                            name="./jcr:language"
                            translateOptions="{Boolean}true">
                            <items jcr:primaryType="nt:unstructured">
                                <japanese
                                    jcr:primaryType="nt:unstructured"
                                    text="Japanese"
                                    value="ja"/>
                                <english
                                    jcr:primaryType="nt:unstructured"
                                    text="English"
                                    value="en"/>
                            </items>
                        </language>
                        <canonicalURL
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                            fieldLabel="Page-Canonical URL"
                            fieldDescription="Page-Canonical URL description"
                            rootPath="/content"
                            name="./canonicalURL"/>
                        <alternateURL
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                            fieldLabel="Page-Alternate URL SP Page"
                            fieldDescription="Page-Alternate URL SP Page description"
                            rootPath="/content"
                            name="./alternateURL"/>
                        <sitename
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="Page-Site Name"
                            fieldDescription="Page-Site Name description"
                            value="マルイ"
                            name="./sitename"/>
                    </items>
                </basic>
                <additionalHTML
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Page-Other HTML Tags"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <headerFreeHTML
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                            fieldLabel="Page-Header Free HTML"
                            fieldDescription="Page-Header Free HTML description"
                            name="./headerFreeHTML"/>
                        <footerFreeHTML
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                            fieldLabel="Page-Footer Free HTML"
                            fieldDescription="Page-Footer Free HTML description"
                            name="./footerFreeHTML"/>
                        <enableNoCacheTag
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                            name="./enableNoCacheTag"
                            text="Page-Output Tags For Clearing The Cache"
                            fieldDescription="Page-Output Tags For Clearing The Cache description"
                            value="true"/>
                    </items>
                </additionalHTML>
                <ogTag
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Page-og Tag"
                    fieldDescription="Page-og Tag description"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <ogTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="og:title"
                            fieldDescription="Page-og title description"
                            name="./ogTitle"/>
                        <ogDescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                            fieldLabel="og:description"
                            fieldDescription="Page-og description description"
                            name="./ogDescription"/>
                        <ogType
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="og:type"
                            fieldDescription="Page-og type description"
                            name="./ogType"/>
                        <ogUrl
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="og:url"
                            fieldDescription="Page-og url description"
                            name="./ogUrl"/>
                        <ogImage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldLabel="og:image"
                            fieldDescription="Page-og image description"
                            rootPath="/content/dam"
                            name="./ogImage"/>
                        <ogSiteName
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                            fieldLabel="og:site_name"
                            fieldDescription="Page-og site name description"
                            name="./ogSiteName"/>
                    </items>
                </ogTag>
            </items>
        </column>
    </items>
</jcr:root>
