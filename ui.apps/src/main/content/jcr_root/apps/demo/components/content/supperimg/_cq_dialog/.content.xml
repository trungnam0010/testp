<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Supper Image"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic Setting"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/uicomponents/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            	 <componentID
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/textfield"
	                            class="halfsizeck"
	                            fieldLabel="Common-Component ID"
	                            fieldDescription="Image-ComponentID Description"
	                            name="./componentID"/>
	                        <image
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Image-ImagePath"
                                fieldDescription="Image-ImagePath Description"
                                name="./fileReference"
                                rootPath="/content/dam"
                                required="{Boolean}true"
                                wrapperClass="urlck"/>
                            <alt
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/textfield"
	                            fieldLabel="Image-Alt"
	                            fieldDescription="Image-Alt Description"
	                            name="./alt"/>
	                        <caption
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/textfield"
	                            fieldLabel="Caption"
	                            fieldDescription="Image-Caption Description"
	                            name="./caption"/>
	                        <linkURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Image-LinkURL"
                                fieldDescription="Image-LinkURL Description"
                                name="./linkURL"
                                class="urlck"/>
                            <linkTitle
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/textfield"
	                            fieldLabel="Image-LinkTitle"
	                            fieldDescription="Image-LinkTitle Description"
	                            name="./linkTitle"/>
	                        <openNewWindow
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Image-OpenLinkInNewWindow Description"
                                name="./openNewWindow"
                                text="Common-OpenLinkInNewWindow"
                                value="true"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Display Setting"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                          <!--   <cssclass
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/textfield"
	                            fieldLabel="Image-CssClass"
	                            fieldDescription="Image-CssClass Description"
	                            name="./cssclass"
	                            class="cssck"/>
	                        <align
                                jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/foundation/form/select"
	                            fieldLabel="Image-Align"
	                            fieldDescription="Image-Align Description"
	                            name="./align">
	                                <items jcr:primaryType="nt:unstructured">
	                                    <left
	                                        jcr:primaryType="nt:unstructured"
	                                        text="_left"
	                                        value="left"/>
	                                    <center
	                                        jcr:primaryType="nt:unstructured"
	                                        text="_center"
	                                        value="center"/>
	                                    <right
	                                        jcr:primaryType="nt:unstructured"
	                                        text="_right"
	                                        value="right"/>
	                                 </items>
                            </align>
                            <enableHoverEffect
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./enableHoverEffect"
                                text="Image-HoverEffect"
                                fieldDescription="Image-HoverEffect Description"
                                value="true"/>
                            <enableLazyLoad
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./enableLazyLoad"
                                text="Image-LazyLoad"
                                fieldDescription="Image-LazyLoad Description"
                                value="true"/> -->
                        </items>
                    </columns>
                </items>
            </tab2>
        </items>
    </content>
</jcr:root>
