<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured"
	jcr:title="Properties" sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/foundation/container">
		<layout jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
		<items jcr:primaryType="nt:unstructured">
			<column jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/foundation/container">
				<items jcr:primaryType="nt:unstructured">
					<template sling:resourceType="granite/ui/components/foundation/form/select"
						jcr:primaryType="nt:unstructured" fieldLabel="Template" multiple="{Boolean}false"
						name="./template">
						<datasource jcr:primaryType="nt:unstructured"
							sling:resourceType="/bin/templates" addNone="{Boolean}true" />
					</template>
					<header sling:resourceType="granite/ui/components/foundation/form/select"
						jcr:primaryType="nt:unstructured" fieldLabel="Select" multiple="{Boolean}false"
						name="./header">
						<items jcr:primaryType="nt:unstructured">
							<option1 jcr:primaryType="nt:unstructured" text="H1"></option1>
							<option2 jcr:primaryType="nt:unstructured" text="H2"></option2>
							<option3 jcr:primaryType="nt:unstructured" text="H5"></option3>
						</items>
					</header>
					<select sling:resourceType="granite/ui/components/foundation/form/select"
						jcr:primaryType="nt:unstructured" fieldLabel="Select" multiple="{Boolean}false"
						name="./select">
						<items jcr:primaryType="nt:unstructured">
							<option1 jcr:primaryType="nt:unstructured" text="69"></option1>
							<option2 jcr:primaryType="nt:unstructured" text="696"></option2>
						</items>
					</select>
					<color sling:resourceType="granite/ui/components/foundation/form/colorpicker"
						jcr:primaryType="nt:unstructured" fieldLabel="Color" multiple="{Boolean}false"
						name="./color">
					</color>
				</items>
			</column>
		</items>
	</content>
</jcr:root>
